;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(extended-command-history file-name-history evil-ex-history))
(setq extended-command-history '("load-file"))
(setq file-name-history '("~/.config/emacs/config.el"))
(setq evil-ex-history '("q!" "wq" "q" "q!" "wq"))
(setq evil-jumps-history 'nil)
